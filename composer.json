{
    "name": "mixislv/laravel-mandrill-driver",
    "description": "Laravel 9+ Mandrill Driver & Webhooks handler",
    "homepage": "https://github.com/mixisLv/laravel-mandrill-driver",
    "keywords":
    [
        "laravel",
        "mandrill",
        "mail",
        "driver",
        "webhooks"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikus Rozenbergs",
            "email": "info@mixis.lv",
            "homepage": "https://mixis.lv"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "illuminate/routing": "^9.0",
        "symfony/mailchimp-mailer": "^6.0",
        "symfony/http-client": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "mixisLv\\LaravelMandrillDriver\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mixisLv\\LaravelMandrillDriver\\MandrillServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
